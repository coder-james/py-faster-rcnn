train_net: "models/pascal_voc/VGG16/faster_rcnn_end2end/ccf_train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 2000
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 20000
# We still use the snapshot prefix, though
snapshot_prefix: "dy1013_vgg16_frcnn"
iter_size: 2
