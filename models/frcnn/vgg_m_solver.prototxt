train_net: "models/pascal_voc/VGG_CNN_M_1024/faster_rcnn_end2end/ccf_train.prototxt"
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 2000
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "24_dy1011_7_faster_rcnn_m"
